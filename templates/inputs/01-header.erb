# This file is managed with puppet.
#
[default]
<% if @host %>
host = <%= @host -%>
<% end -%>
<% if @index %>
index = <%= @index -%>
<% end -%>
<% if @source %>
source = <%= @source -%>
<% end -%>
<% if @sourcetype %>
sourcetype = <%= @sourcetype -%>
<% end -%>
<% if @queue %>
queue = <%= @queue -%>
<% end -%>
<% if @tcp_routing -%>
_TCP_ROUTING = <%= @tcp_routing.join(',') -%>
<% end -%>
<% if @syslog_routing -%>
_SYSLOG_ROUTING = <%= @syslog_routing.join(',') -%>
<% end -%>
<% if @index_and_forward_routing -%>
_INDEX_AND_FORWARD_ROUTING = <%= @index_and_forward_routing -%>
<% end -%>
<% if @custom_hash -%>
  <% @custom_hash.sort.each do |key,value| -%>
<%= key -%> = <%= value %>
  <% end -%>
<% end -%>

